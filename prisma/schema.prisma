generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movement {
  id                 String      @unique @default(uuid())
  initial_position   String
  movements_to_do    String
  movements_done     String[]
  final_position     String
  created_at         DateTime    @default(now())
}
